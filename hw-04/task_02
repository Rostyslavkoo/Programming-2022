#include <bits/stdc++.h>

using namespace std;

#define arrLength 1005
int
main ()
{

  long double a[arrLength];
  a[1] = 0.01;

  for (int i = 2; i <= arrLength - 5; i++)
    {
      a[i] = sin (i + a[i - 1]);
    }
  int k = 0;
  int sum = 0;
  for (int i = 1; i <= arrLength; i = pow (2, k))
    {
      if (a[i] < 0.25)
	{
	  sum++;

	}
      k++;
    }
  cout << sum;


  return 0;
}
